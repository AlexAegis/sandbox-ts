  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.1",
    "@types/workerpool": "^3.1.0",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.13",
    "ts-node": "8.3.0",
    "tsconfig-paths": "^3.8.0",
