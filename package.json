{
	"name": "@alexaegis/ts-sandbox",
	"displayName": "Sandbox",
	"description": "Sandbox Project",
	"version": "1.0.0",
	"author": {
		"name": "Alex Aegis",
		"email": "alexaegis@gmail.com",
		"url": "https://github.com/AlexAegis"
	},
	"homepage": "https://github.com/AlexAegis",
	"bugs": {
		"email": "alexaegis@gmail.com",
		"url": "https://github.com/AlexAegis/ts-sandbox/issues"
	},
	"scripts": {
		"lint": "eslint --max-warnings 0 ./**/*.{ts,js} ",
		"lint:fix": "eslint --max-warnings 0 ./**/*.{ts,js} --fix",
		"format": "npx prettier --write \"**/*.{ts,js}\"",
		"prettier": "npm run format",
		"test": "mocha --config ./.mocharc",
		"test:coverage": "nyc npm test",
		"ncu": "ncu -u",
		"codecov": "codecov",
		"clean": "rimraf node_modules package-lock.json",
		"postinstall": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/AlexAegis/advent-of-code/"
	},
	"categories": [
		"Other"
	],
	"license": "mit",
	"private": true,
	"dependencies": {
		"moment": "^2.29.1",
		"rxjs": "^7.5.5",
		"workerpool": "^6.2.0"
	},
	"devDependencies": {
		"@types/chai": "^4.3.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "^17.0.23",
		"@types/workerpool": "^6.1.0",
		"@typescript-eslint/eslint-plugin": "^5.17.0",
		"@typescript-eslint/parser": "^5.17.0",
		"chai": "^4.3.6",
		"codecov": "^3.8.2",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsdoc": "^38.1.4",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.29.4",
		"husky": "^7.0.4",
		"mocha": "^9.2.2",
		"npm-check-updates": "^12.5.5",
		"nyc": "^15.1.0",
		"pinst": "^3.0.0",
		"prettier": "^2.6.1",
		"source-map-support": "^0.5.21",
		"ts-node": "10.7.0",
		"tsconfig-paths": "^3.14.1",
		"typescript": "4.6.3"
	},
	"nyc": {
		"extension": [
			".ts",
			".tsx"
		],
		"include": [
			"src/**/*.ts"
		],
		"exclude": [
			"node_modules/",
			"src/**/*.d.ts",
			"src/**/*.spec.ts",
			"src/**/deque.class.ts"
		],
		"reporter": [
			"lcov"
		],
		"all": true
	}
}
